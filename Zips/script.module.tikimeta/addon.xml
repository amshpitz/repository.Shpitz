<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.module.tikimeta" version="1.2.10" name="Tiki Meta Module" provider-name="Tiki">
    <requires>
        <import addon="script.module.requests" version="2.19.1" />
        <import addon="xbmc.python" version="2.19.0"/>
    </requires>
    <extension library="lib/default.py" point="xbmc.python.pluginsource">
        <provides>executable</provides>
    </extension>
    <extension library="lib/services.py" point="xbmc.service" start="startup" />
    <extension library="lib" point="xbmc.python.module" />
    <extension point="xbmc.addon.metadata">
        <reuselanguageinvoker>true</reuselanguageinvoker>
        <summary>Metadata for Addons</summary>
        <description>Metadata for Addons</description>
        <platform>all</platform>
        <license>GNU GENERAL PUBLIC LICENSE. Version 3, 29 June 2007</license>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.png</fanart>
        </assets>
        <news>
        </news>
    </extension>
</addon>
